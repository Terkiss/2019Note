package com.terukiss.mytextviewlibrary;

import android.widget.Button;

public class SelfButton {
    private String Id;
    private Button button;

    public String getId() {
        return Id;
    }

    public void setId(String id) {
        Id = id;
    }

    public Button getButton() {
        return button;
    }

    public void setButton(Button button) {
        this.button = button;
    }
}
